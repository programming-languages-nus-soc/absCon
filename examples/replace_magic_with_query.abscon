#abstract
{
	#concVec vec;
	#abstVec abst;

	#assigns {
        setFeatures(abst, s_load) = 0;
        setFeatures(abst, s_store) = 0;
	}
}


#concrete{
    #clone vec;
    #query veq;
    	#filters{
            exists vi in inlinedInto(vec):{
                sourceOfSize(vi)(2) != 0 && sourceOf(vi)(9)(2) + sourceOf(vi)(13)(2)+sourceOf(vi)(34)(2)+sourceOf(vi)(45)(2) == 1
                && sourceOf(vi)(41)(2) == 1;
            }
    	}
